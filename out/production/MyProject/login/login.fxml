<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="principalbox" prefHeight="400.0" prefWidth="350.0"
            xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="login.LoginDriver">
   <children>
      <Label layoutX="28.0" layoutY="27.0" text="DB status" />
      <Label fx:id="dbstatus" layoutX="106.0" layoutY="28.0" prefHeight="18.0" prefWidth="116.0" />
      <Label layoutX="28.0" layoutY="78.0" prefHeight="18.0" prefWidth="86.0" text="Username" />
      <TextField fx:id="username" layoutX="106.0" layoutY="74.0" prefHeight="28.0" prefWidth="219.0" promptText="username" />
      <Label layoutX="28.0" layoutY="122.0" prefHeight="18.0" prefWidth="86.0" text="Password" />
      <PasswordField fx:id="password" layoutX="106.0" layoutY="118.0" prefHeight="26.0" prefWidth="219.0" promptText="password" />
      <ComboBox fx:id="seleccionbox" layoutX="106.0" layoutY="178.0" prefHeight="28.0" prefWidth="219.0" promptText="Admin / User" />
      <Button fx:id="loginbutt" layoutX="106.0" layoutY="237.0" mnemonicParsing="false" onAction="#Login" prefHeight="32.0" prefWidth="219.0" text="Login" />
      <Label fx:id="loginStatus" layoutX="106.0" layoutY="299.0" prefHeight="18.0" prefWidth="219.0" />
   </children>
</AnchorPane>
